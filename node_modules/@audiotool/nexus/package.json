{
  "name": "@audiotool/nexus",
  "version": "0.0.6",
  "type": "module",
  "license": "MIT",
  "description": "Audiotool Nexus SDK",
  "author": "Audiotool Inc.",
  "homepage": "https://developer.audiotool.com/js-package-documentation/",
  "keywords": [
    "audiotool",
    "nexus",
    "real-time",
    "collaboration",
    "synced document",
    "DAW",
    "music",
    "production",
    "audio"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./document": {
      "types": "./dist/document.d.ts",
      "import": "./dist/document.js"
    },
    "./entities": {
      "types": "./dist/entities.d.ts",
      "import": "./dist/entities.js"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "import": "./dist/utils.js"
    },
    "./api": {
      "types": "./dist/api.d.ts",
      "import": "./dist/api.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && npm pack --pack-destination dist/",
    "dev": "vite build --watch",
    "docs": "typedoc",
    "test-docs": "cd docs && uv run python -m http.server",
    "validator": "cd ../hermes && just wasm && cd - && cp ../hermes/document_validator_wasm/document_validator.wasm src/document/mock/wasm/document_validator.wasm",
    "codegen": "buf generate --template codegen/proto.buf.gen.yaml && buf generate --template codegen/nexus.buf.gen.yaml && buf generate --template codegen/proto2.buf.gen.yaml && node codegen/scripts/generate-protobuf-exports.js && node codegen/scripts/generate-protobuf-exports-services.js"
  },
  "devDependencies": {
    "@8hobbies/typedoc-plugin-plausible": "^2.2.0",
    "@bufbuild/buf": "^1.56.0",
    "@bufbuild/protoplugin": "1.10.0",
    "@microsoft/api-documenter": "^7.26.32",
    "@microsoft/api-extractor": "^7.52.11",
    "@prettier/sync": "^0.6.1",
    "@types/node": "^24.3.0",
    "@types/toposort": "^2.0.7",
    "@types/uuid": "^10.0.0",
    "glob": "^11.0.3",
    "tsx": "^4.0.0",
    "typedoc": "^0.28.10",
    "typedoc-plugin-mdn-links": "^5.0.8",
    "typedoc-plugin-missing-exports": "^4.1.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.0.0",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@bufbuild/protobuf": "1.10.0",
    "@connectrpc/connect": "^1.4.0",
    "@connectrpc/connect-web": "^1.4.0",
    "@types/js-cookie": "^3.0.6",
    "js-cookie": "^3.0.5",
    "toposort": "^2.0.2",
    "utility-types": "^3.11.0",
    "uuid": "^11.1.0"
  }
}
